{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-no-lambda": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-string-literal": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "assets/*/*/*.js",
      "public/assets/*/*/*.js"
    ]
  }
}
